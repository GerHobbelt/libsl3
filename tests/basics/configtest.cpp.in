#include <sl3/config.hpp>
#include <a4testing.hpp>

namespace sl3 {
  namespace {


    void
    CheckSqliteVersion ()
    {
      BOOST_WARN_MESSAGE(build_internal_sqlite3, "not build_internal_sqlite3");

      if(build_internal_sqlite3)
        {
          using namespace sl3;
          BOOST_CHECK_EQUAL(
              sqliteCompiledVersion() , sqliteRuntimeVersion()
          );

          BOOST_CHECK(
              sqliteCompiledVersionNumber() == sqliteRuntimeVersionNumber()
          );
          
          // check that the cmake file was updated correct
          std::string expectedVersion =
              "3." + std::to_string(sl3::MINOR_VERSION) + "." +
              std::to_string(sl3::PATCH_VERSION) ;

          // check that the cmake file was updated correct
          BOOST_CHECK(expectedVersion == sqliteRuntimeVersion()) ;
                    
        }
    }


    a4TestAdd(
      a4test::suite("config")
      .addTest("check version", CheckSqliteVersion)

    ) ;
  }
}

